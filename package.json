{
  "name": "react-native-typescript-boilerplate",
  "description": "An Awesome React Native TypeScript Boilerplate",
  "version": "0.0.1",
  "homepage": "https://wuud-team.com",
  "author": {
    "name": "Med Redha Khelifi",
    "email": "redha@wuud-team.com",
    "url": "https://github.com/MedRedha"
  },
  "scripts": {
    "clean": "watchman watch-del-all && watchman watch ./ && rm -rf /Users/MedRedha/Library/Developer/Xcode/DerivedData/",
    "start": "react-native start --reset-cache",
    "ios": "(cd ios && xcodebuild -alltargets clean) && npx react-native run-ios --simulator=\"iPhone 12 Pro Max\"",
    "android": "npx react-native run-android --variant=devDebug",
    "pod": "npx pod-install",
    "pod update": "(cd ios && pod repo update)",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "npx jetify",
    "release:android": "cd android && ./gradlew assembleRelease",
    "clean:android": "cd android && ./gradlew clean",
    "release:play": "cd android && ./gradlew bundleRelease",
    "test:apk": "react-native run-android --variant=release"
  },
  "dependencies": {
    "@freakycoder/react-native-helpers": "^1.0.1",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "axios": "^0.21.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "events": "^3.3.0",
    "lottie-ios": "3.1.8",
    "lottie-react-native": "^3.5.0",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.63.2",
    "react-native-animatable": "^1.3.3",
    "react-native-bouncy-touchable": "^1.0.0",
    "react-native-dynamic-vector-icons": "^1.1.6",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-global-props": "^1.1.5",
    "react-native-ios-kit": "^0.0.22",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localization": "^2.1.7",
    "react-native-mmkv-storage": "^0.6.0",
    "react-native-orientation-locker": "^1.3.1",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-vector-icons": "^8.1.0",
    "react-navigation-helpers": "^1.1.1",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/runtime": "^7.14.6",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.3.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.js --fix-dry-run"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  }
}
